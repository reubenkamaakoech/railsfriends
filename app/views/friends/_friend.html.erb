<div id="<%= don_id friend %>" ></div>

  <div class="jumbotron">
  <h1 class="display-4">
    <%= friend.first_name %>
    <%= friend.middle_name %>
    <%= friend.last_name %>
  </h1>
  <p class="lead">
    <%= friend.email %> | 
    <%= friend.phone %> | 
    <%= friend.twitter %>
  </p>
 
  <a href="#" role="button">
    <%= link_to "Edit", edit_friend_path(@friend), class:"btn btn-outline-primary" %>
  <%= link_to "Back", friends_path(@friend), class:"btn btn-outline-primary" %>
  </a>
</div>